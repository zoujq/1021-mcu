              //===================================================================================================
              void CTK_Debug_Mode_Uart_SendChar(u8 send_data)
              {
                SBUF = send_data;
              
                while(!TI);
              
                TI = 0;
              }
              
              //===================================================================================================
              void CTK_Debug_Mode_Uart_SendWord(u16 send_data)
              {
                SBUF = (u8)(send_data >> 8);
              
                while(!TI);
              
                TI = 0;
              
                SBUF = (u8)send_data;
              
                while(!TI);
              
                TI = 0;
              }
              
